{
  "name": "amperize",
  "version": "0.6.1",
  "description": "AMP up your plain HTML",
  "main": "./lib/amperize",
  "scripts": {
    "test": "mocha $(find test -name '*.test.js')",
    "watch": "mocha --watch $(find test -name '*.test.js')",
